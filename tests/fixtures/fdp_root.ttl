@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fdp: <https://w3id.org/fdp/fdp-o#> .
@prefix ldp: <http://www.w3.org/ns/ldp#> .

<https://example.org/fdp>
    a fdp:FAIRDataPoint, dcat:DataService ;
    dct:title "Example FAIR Data Point" ;
    dct:description "A sample FAIR Data Point for testing purposes. Contains research datasets from Example University." ;
    dct:publisher <https://example.org/organization> ;
    rdfs:label "Example FAIR Data Point" ;
    fdp:metadataCatalog <https://example.org/fdp/catalog/research-data> ;
    ldp:contains <https://example.org/fdp/catalog/research-data> .

<https://example.org/organization>
    a foaf:Organization ;
    foaf:name "Example University" .
